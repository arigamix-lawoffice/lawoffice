[TEXTPART Documents MSSQL]
SELECT
	#if(Normal) {
	[t].[ID]																	AS [DocID],
	[t].[FullNumber]															AS [DocNumber],
	[t].[DocTypeTitle]															AS [SubTypeTitle],
	[t].[Subject]																AS [DocSubject],
	COALESCE([t].[FullNumber], N'') + N', ' + COALESCE([t].[Subject], N'')		AS [DocDescription], 	-- autocomplete
	[t].[AuthorID]																AS [AuthorID],
	[t].[AuthorName]															AS [AuthorName],
	[t].[RegistratorID]															AS [RegistratorID],
	[t].[RegistratorName]														AS [RegistratorName],
	[t].[StateName]																AS [KrState],
	[t].[DocDate]																AS [DocDate],
	CAST([t].[CreationDate] AS date)											AS [CreationDate],
	[t].[DepartmentName]														AS [Department]
	}
	#if(ByDocType) {
	[t].[SubTypeID],
	[kdt].[Title]																AS [SubTypeTitle],
	[t].[SubTypeCount]
	}
	#if(ByState || Count) {
	[t].*
	}
FROM (
	SELECT
		#if(Normal) {
		[mi].[ID],
		[mi].[FullNumber],
		[mi].[DocTypeTitle],
		[mi].[Subject],
		[mi].[AuthorID],
		[mi].[AuthorName],
		[mi].[RegistratorID],
		[mi].[RegistratorName],
		[mi].[StateName],
		[mi].[DocDate],
		[mi].[CreationDate],
		[mi].[DepartmentName],
		[t].[rn]
		}
		#if(ByDocType || ByState) {
		[t].*
		}
		#if(Count) {
		count(*) AS [cnt]
		}
	FROM (
		SELECT
			#if(Normal || Count) {
			[mi].[ID]
			}
			#if(ByDocType) {
			[mi].[DocTypeID]									AS [SubTypeID],
			count(*)											AS [SubTypeCount]
			}
			#if(ByState) {
			[mi].[StateID]										AS [StateID],
			[mi].[StateName]									AS [StateName],
			count(*)											AS [StateCount]
			}
			#if(Normal) {,
			row_number() OVER (ORDER BY #order_by) AS [rn]
			}
		FROM [DocumentCommonInfo] AS [mi] WITH (NOLOCK)
		#if(IsInitiator) {
		LEFT JOIN [KrApprovalCommonInfo] AS [kr] WITH (NOLOCK)
			ON [kr].[MainCardID] = [mi].[ID]
		}
		WHERE
			[mi].[CardTypeID] = '6d06c5a0-9687-4f6b-9bed-d3a081d84d9a'		--Document
			#param(Number, [mi].[FullNumber])
			#param(Subject, [mi].[Subject])
			#param(DocDate, [mi].[DocDate])
			#param(State, [mi].[StateID])
			#param(DocType, [mi].[DocTypeID])
			#param(Author, [mi].[AuthorID])
			#param(Registrator, [mi].[RegistratorID])
			#param(Department, [mi].[DepartmentID])
			#if(IsAuthor) {
				#if(IsAuthor.CriteriaName == "IsTrue") {
			AND [mi].[AuthorID] = #param(CurrentUserID)
				} {
			AND ([mi].[AuthorID] IS NULL OR [mi].[AuthorID] <> #param(CurrentUserID))
				}
			}
			#if(IsRegistrator) {
				#if(IsRegistrator.CriteriaName == "IsTrue") {
			AND [mi].[RegistratorID] = #param(CurrentUserID)
				} {
			AND ([mi].[RegistratorID] IS NULL OR [mi].[RegistratorID] <> #param(CurrentUserID))
				}
			}
			#if(IsInitiator) {
				#if(IsInitiator.CriteriaName == "IsTrue") {
			AND [kr].[AuthorID] = #param(CurrentUserID)
				} {
			AND ([kr].[AuthorID] IS NULL OR [kr].[AuthorID] <> #param(CurrentUserID))
				}
			}
		#if(ByDocType) {
		GROUP BY [mi].[DocTypeID]
		}
		#if(ByState) {
		GROUP BY [mi].[StateID], [mi].[StateName]
		}
		) AS [t]
	#if(Normal) {
	INNER JOIN [DocumentCommonInfo] AS [mi] WITH (NOLOCK)
		ON [mi].[ID] = [t].[ID]
	}
	#if(PageOffset) {
	WHERE [t].[rn] >= #param(PageOffset) AND [t].[rn] < (#param(PageOffset) + #param(PageLimit))
	}
	) AS [t]
#if(Normal) {
ORDER BY [t].[rn]
}
#if(ByState) {
CROSS APPLY [Localization]([t].[StateName], #param(locale)) AS [sls] --Локализуем состояние документа
ORDER BY [sls].[Value]
}
#if(ByDocType) {
INNER JOIN [KrDocType] AS [kdt] WITH (NOLOCK)
	ON [kdt].[ID] = [t].[SubTypeID]
CROSS APPLY [Localization]([kdt].[Title], #param(locale)) AS [tls] --Локализуем тип документа
ORDER BY [tls].[Value]
}
;


